{
	"name": "aramoks-clearcase",
	"displayName": "Aramok's Clearcase",
	"description": "IBM Rational Clearcase integration with Visual Studio Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.checkout",
				"title": "Cleartool: Checkout"
			},
			{
				
				"command": "extension.checkin",
				"title": "Cleartool: Checkin"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.checkout",
					"group": "Aramok's Commands",
					"when":"checkout-criteria"
				},
				{
					"command": "extension.checkin",
					"group": "Aramok's Commands",
					"when":"checkin-criteria"
				}
			],
			"editor/context": [
				{
					"command": "extension.checkout",
					"group": "Aramok's Commands",
					"when":"checkout-criteria"
				},
				{
					"command": "extension.checkin",
					"group": "Aramok's Commands",
					"when":"checkin-criteria"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}